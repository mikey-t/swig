{
  "name": "swig-example-typescript-tranpiled",
  "version": "1.0.0",
  "description": "Example swig usage with an ESM project (package.json) and a typescript swigfile.ts that you transpile to ESM swigfile.js whenever you have changes.",
  "type": "module",
  "scripts": {
    "transpileSwigfile": "swc swigfile.ts -o swigfile.js",
    "transpileSwigfileTsc": "tsc --target ESNext --module NodeNext --moduleResolution NodeNext swigfile.ts"
  },
  "keywords": [],
  "author": "Mike Thompson",
  "license": "MIT",
  "volta": {
    "node": "18.17.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "swig-cli": "file:../../packed/swig-cli-0.0.12.tgz",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
