{
  "name": "swig-example-typescript-transpiled",
  "version": "1.0.0",
  "description": "Swig-cli example. ESM project, ESM swigfile.ts with an extra build step to transpile swigfile.ts to swigfile.js.",
  "type": "module",
  "scripts": {
    "transpileSwigfile": "swc swigfile.ts -o swigfile.js",
    "transpileSwigfileTsc": "tsc --target ESNext --module NodeNext --moduleResolution NodeNext swigfile.ts"
  },
  "author": "Mike Thompson",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.13",
    "swig-cli": "file:..\\..\\packed\\swig-cli-1.0.5-alpha.6.tgz",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core"
    ]
  }
}
